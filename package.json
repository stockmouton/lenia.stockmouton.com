{
  "name": "lenia.stockmouton.com",
  "private": true,
  "description": "The homepage of the Lenia NFT collection",
  "version": "0.1.0",
  "author": "Alex Orange <alex.orange@stockmouton.com>",
  "dependencies": {
    "@assemblyscript/loader": "^0.19.17",
    "@openzeppelin/contracts": "^4.3.1",
    "@walletconnect/web3-provider": "^1.6.5",
    "axios": "^0.22.0",
    "babel-plugin-styled-components": "^1.13.2",
    "beautiful-react-hooks": "^1.0.1",
    "buffer": "^6.0.3",
    "d3": "^7.0.3",
    "ethers": "^5.4.6",
    "gatsby": "^3.11.1",
    "gatsby-plugin-google-gtag": "^3.14.0",
    "gatsby-plugin-image": "^1.11.0",
    "gatsby-plugin-manifest": "^3.11.0",
    "gatsby-plugin-offline": "^4.11.0",
    "gatsby-plugin-react-helmet": "^4.11.0",
    "gatsby-plugin-sharp": "^3.11.0",
    "gatsby-plugin-styled-components": "^4.12.0",
    "gatsby-plugin-use-query-params": "^1.0.1",
    "gatsby-source-filesystem": "^3.11.0",
    "gatsby-transformer-sharp": "^3.11.0",
    "lodash": "^4.17.21",
    "node-gzip": "^1.1.2",
    "pako": "^2.0.4",
    "process": "^0.11.10",
    "prompt": "^1.2.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-countdown": "^2.3.2",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "sanitize.css": "^12.0.1",
    "styled-components": "^5.3.0",
    "uglify-js": "^3.14.2",
    "use-query-params": "^1.2.3",
    "web3": "^1.5.2",
    "web3modal": "^1.9.4"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "npm:hardhat-deploy-ethers",
    "@nomiclabs/hardhat-etherscan": "^2.1.6",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "assemblyscript": "^0.19.17",
    "chai": "^4.3.4",
    "dotenv": "^10.0.0",
    "ethereum-waffle": "^3.4.0",
    "gatsby-plugin-env-variables": "^2.1.0",
    "gh-pages": "^3.2.3",
    "hardhat": "^2.6.1",
    "hardhat-deploy": "^0.9.1",
    "hardhat-gas-reporter": "^1.0.4",
    "ndb": "^1.1.5",
    "prettier": "2.3.2",
    "solidity-coverage": "^0.7.17",
    "webpack": "^5.51.1"
  },
  "keywords": [
    "lenia",
    "stockmouton"
  ],
  "scripts": {
    "build": "gatsby build",
    "format": "prettier --write \"deploy/**/*.{js,jsx,ts,tsx,json,md}\" \"src/**/*.{js,jsx,ts,tsx,json,md}\" \"test/**/*.{js,jsx,ts,tsx,json,md}\"",
    "develop": "gatsby develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "yarn sm-test",
    "deploy": "gatsby clean && gatsby build && gh-pages -d public -b prod",
    "local-node": "hardhat node",
    "sm-compile": "hardhat compile",
    "sm-test": "hardhat test",
    "sm-coverage": "hardhat coverage",
    "sm-deploy": "hardhat deploy --network ${NETWORK:-localhost} --export src/artifacts.json --tags 'Lenia'",
    "sm-export": "hardhat export --network ${NETWORK:-localhost} --export src/artifacts.json",
    "sm-set-baseuri": "hardhat set-baseuri --network ${NETWORK:-localhost}",
    "sm-add-presale-list": "hardhat add-presale-list --network ${NETWORK:-localhost}",
    "sm-eligible-presale": "hardhat eligible-presale --network ${NETWORK:-localhost}",
    "sm-start-presale": "hardhat start-presale --network ${NETWORK:-localhost}",
    "sm-start-sale": "hardhat start-sale --network ${NETWORK:-localhost}",
    "sm-set-engine": "hardhat set-engine --network ${NETWORK:-localhost}",
    "sm-get-engine": "hardhat get-engine --network ${NETWORK:-localhost}",
    "sm-set-leniaparams": "hardhat set-leniaparams --network ${NETWORK:-localhost}",
    "sm-get-leniaparams": "hardhat get-leniaparams --network ${NETWORK:-localhost}",
    "sm-set-metadata": "hardhat set-metadata --network ${NETWORK:-localhost}",
    "sm-populate-all": "hardhat populate-all --network ${NETWORK:-localhost}",
    "sm-claim-reserved": "hardhat claim-reserved --network ${NETWORK:-localhost}",
    "sm-estimate-gas": "REPORT_GAS=true hardhat test",
    "sm-reveal": "hardhat reveal --network ${NETWORK:-localhost}",
    "sm-release": "hardhat release --network ${NETWORK:-localhost}",
    "sm-get-shares": "hardhat get-shares --network ${NETWORK:-localhost}",
    "sm-verify-lenia": "hardhat verify-lenia --network ${NETWORK:-localhost}",
    "sm-verify-royalties": "hardhat verify-royalties --network ${NETWORK:-localhost}",
    "asbuild:untouched": "asc src/assembly/engine.ts --target debug",
    "asbuild:optimized": "asc src/assembly/engine.ts --target release",
    "asbuild": "yarn asbuild:untouched && yarn asbuild:optimized",
    "debug": "ndb"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stockmouton/lenia.stockmouton.com"
  }
}
